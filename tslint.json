{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-no-lambda": false,
    "member-access": [
      true,
      "no-public"
    ],
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "semicolon": [
      true,
      "always"
    ],
    "no-console": false,
    "no-string-literal": false
  }
}
